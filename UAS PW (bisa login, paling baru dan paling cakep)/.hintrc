{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": "off",
    "compat-api/html": "off",
    "axe/forms": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "text-decoration-skip-ink",
          "-webkit-tap-highlight-color"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 120",
    "not chrome <= 120",
    "not edge <= 120",
    "not firefox <= 121",
    "not safari <= 17.2",
    "not samsung <= 23"
  ]
}